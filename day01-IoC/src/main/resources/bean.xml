<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="cn.daliandaxue.entity.Person">
        <constructor-arg name="age" value="19" ></constructor-arg>
        <constructor-arg name="name" value="Yanfu"></constructor-arg>
    </bean>
    <bean id="personService" class="cn.daliandaxue.service.PersonService"></bean>
    <bean id="personDao" class="cn.daliandaxue.dao.PersonDao"></bean>
    <bean id="mainConfig" class="cn.daliandaxue.config.MainConfig"></bean>
</beans>